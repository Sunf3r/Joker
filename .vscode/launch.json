	"version": "0.2.0",
	"configurations": [
		{
			"request": "launch",
			"name": "DENO",
			"type": "node",
			"program": "${workspaceFolder}/Updater.ts",
			"cwd": "${workspaceFolder}",
			"runtimeExecutable": "C:\\Users\\%USERPROFILE%\\.deno\\bin\\deno.EXE",
			"args": ["--dev"],
			"runtimeArgs": [
				"run",
				"--inspect-wait",
				"--allow-all"
			],
			"attachSimplePort": 9229
		}
	]
}
